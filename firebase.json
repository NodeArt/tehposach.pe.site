{
  "hosting": {
    "headers": [
      {
        "source": "/",
        "headers": [
          {
            "key": "Link",
            "value": "</js/app.js>;rel=preload;as=script,</css/styles.css>;rel=preload;as=style"
          }
        ]
      }
    ],
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/sendMail",
        "function": "sendMail"
      }
    ],
    "cleanUrls": true,
    "trailingSlash": false
  }
}
